        "nplin",
        "pipx",
        "pmatrix",
        "pmax",
        "pyproj",
        "pyproject",
        "pytest",
        "pyupgrade",
        "rtol",
        "susceptance",
        "transfo",
        "ureg",
        "yesqa"
